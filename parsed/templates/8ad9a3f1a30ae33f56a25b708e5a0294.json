{
  "components/common/footer": "{{#if banners.bottom}}\r\n    <div class=\"banners\" data-banner-location=\"bottom\">\r\n        {{#each (limit banners.bottom_metadata 1)}}\r\n            <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n                <div data-event-type=\"promotion-click\">\r\n                    {{{this.content}}}\r\n                </div>\r\n            </div>\r\n        {{/each}}\r\n    </div>\r\n{{/if}}\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\r\n    <div class=\"container\">\r\n        {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\r\n            <article class=\"footer-info-col\r\n                footer-info-col--social\r\n                footer-info-col--{{#if theme_settings.social_icon_placement_bottom '===' 'bottom_left'}}left{{else}}right{{/if}}\"\r\n                data-section-type=\"footer-webPages\">\r\n                    <h3 class=\"footer-info-heading\">{{lang 'social.connect'}}</h3>\r\n                    {{> components/common/social-links}}\r\n            </article>\r\n        {{/if}}\r\n        <section class=\"footer-info\">\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                <h3 class=\"footer-info-heading\">{{lang 'footer.navigate'}}</h3>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#each pages}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                    {{#if settings.phone_number}}\r\n                    <a href=\"#\">{{settings.phone_number}}</a>\r\n                {{/if}}\r\n                    <!-- <li>\r\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                    </li> -->\r\n                </ul>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\r\n                <h3 class=\"footer-info-heading\">Company</h3>\r\n                <ul class=\"footer-info-list\">\r\n                    <!-- {{#each categories}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}} -->\r\n                    <li><a href=\"/story\">Our Story</a></li>\r\n                    <li><a href=\"/finishes\">Finishes</a></li>\r\n                    <li><a href=\"/blog\">Blogs</a></li>\r\n                </ul>\r\n            </article>\r\n\r\n            {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-brands\">\r\n                <h3 class=\"footer-info-heading\">About</h3>\r\n                <ul class=\"footer-info-list\">\r\n                    <!-- {{#each shop_by_brand}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}} -->\r\n                    <!-- <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li> -->\r\n                    <li><a href=\"/faq\">FAQs</a></li>\r\n                    <li><a href=\"/ship-policy\">Shipping Policy</a></li>\r\n                    <li><a href=\"/returns\">Return Policy</a></li>\r\n                </ul>\r\n            </article>\r\n            {{/and}}\r\n\r\n            {{#if settings.address}}\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"storeInfo\">\r\n                <h3 class=\"footer-info-heading\">Accounts</h3>\r\n                <!-- <address>{{nl2br settings.address}}</address> -->\r\n                <ul class=\"footer-info-list\">\r\n\r\n                    <li><a href=\"/sign-in\">Sign In</a></li>\r\n                    <li><a href=\"/register\">Register</a></li>\r\n                    <li><a href=\"#pro\">Become a Baldwin Pro</a></li>\r\n                    <li><a href=\"/projects\">My Projects</a></li>\r\n                    <li><a href=\"/order-status\">Order Status</a></li>\r\n                </ul>\r\n            </article>\r\n            {{/if}}\r\n\r\n            <article class=\"footer-info-col\" data-section-type=\"newsletterSubscription\">\r\n                {{#if settings.show_newsletter_box}}\r\n                    {{> components/common/subscription-form}}\r\n                {{/if}}\r\n                {{> components/common/social-links}}\r\n                {{> components/common/payment-icons}}\r\n            </article>\r\n        </section>\r\n        {{#if theme_settings.show_powered_by}}\r\n            <div class=\"footer-copyright\">\r\n                <p class=\"powered-by\">{{lang 'footer.powered_by'}} <a href=\"https://www.bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" rel=\"nofollow\">BigCommerce</a></p>\r\n            </div>\r\n        {{/if}}\r\n        {{#if theme_settings.show_copyright_footer}}\r\n            <div class=\"footer-copyright\">\r\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} </p>\r\n            </div>\r\n        {{/if}}\r\n    </div>\r\n    {{{region name=\"ssl_site_seal--global\"}}}\r\n</footer>\r\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            {{#if display_name '===' 'twitter'}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--x\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"X\"\n                    >\n                        <span class=\"aria-description--hidden\">X</span>\n                        <svg>\n                            <use href=\"#icon-x\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{else}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--{{display_name}}\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"{{{capitalize display_name}}}\"\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                        <svg>\n                            <use href=\"#icon-{{display_name}}\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n{{/if}}\n",
  "components/common/subscription-form": "<h3 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h3>\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\"\n                       id=\"nl_email\"\n                       name=\"nl_email\"\n                       type=\"email\"\n                       value=\"{{customer.email}}\"\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n                       aria-describedby=\"alertBox-message-text\"\n                       aria-required=\"true\"\n                       autocomplete=\"email\"\n                       required\n                >\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\"\n                       type=\"submit\"\n                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\n                >\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n",
  "components/common/payment-icons": "{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay show_accept_klarna}}\n    <ul class=\"footer-payment-icons\" aria-label=\"{{lang 'account.payment_methods.payment_icons_label'}}\">\n        {{#if show_accept_amex}}\n        <li>\n            <svg class=\"footer-payment-icon\" role=\"img\">\n                <title>American Express</title>\n                <use href=\"#icon-logo-american-express\"></use>\n            </svg>\n        </li>\n        {{/if}}\n        {{#if show_accept_discover}}\n        <li>\n            <svg class=\"footer-payment-icon\" role=\"img\">\n                <title>Discover</title>\n                <use href=\"#icon-logo-discover\"></use>\n            </svg>\n        </li>\n        {{/if}}\n        {{#if show_accept_mastercard}}\n        <li>\n            <svg class=\"footer-payment-icon\" role=\"img\">\n                <title>Mastercard</title>\n                <use href=\"#icon-logo-mastercard\"></use>\n            </svg>\n        </li>\n        {{/if}}\n        {{#if show_accept_paypal}}\n        <li>\n            <svg class=\"footer-payment-icon\" role=\"img\">\n                <title>PayPal</title>\n                <use href=\"#icon-logo-paypal\"></use>\n            </svg>\n        </li>\n        {{/if}}\n        {{#if show_accept_visa}}\n        <li>\n            <svg class=\"footer-payment-icon\" role=\"img\">\n                <title>Visa</title>\n                <use href=\"#icon-logo-visa\"></use>\n            </svg>\n        </li>\n        {{/if}}\n        {{#if show_accept_amazonpay}}\n        <li>\n            <svg class=\"footer-payment-icon\" role=\"img\">\n                <title>Amazon Pay</title>\n                <use href=\"#icon-logo-amazonpay\"></use>\n            </svg>\n        </li>\n        {{/if}}\n        {{#if show_accept_googlepay}}\n        <li>\n            <svg class=\"footer-payment-icon\" role=\"img\">\n                <title>Google Pay</title>\n                <use href=\"#icon-logo-googlepay\"></use>\n            </svg>\n        </li>\n        {{/if}}\n        {{#if show_accept_klarna}}\n        <li>\n            <svg class=\"footer-payment-icon\" role=\"img\">\n                <title>Klarna</title>\n                <use href=\"#icon-logo-klarna\"></use>\n            </svg>\n        </li>\n        {{/if}}\n    </ul>\n    {{/or}}\n{{/with}}\n"
}
