{
  "components/category/explore-carousel": "<section class=\"productCarousel popular-section\" \r\n    data-slick='{\r\n        \"infinite\": false,\r\n        \r\n        \"mobileFirst\": true,\r\n        \"slidesToShow\": 2,\r\n        \"slidesToScroll\": 2,\r\n        \"slide\": \"[data-product-slide]\",\r\n        \"responsive\": [\r\n            {\r\n                \"breakpoint\": 800,\r\n                \"settings\": {\r\n                    \"slidesToShow\": 4,\r\n                    \"slidesToScroll\": 1\r\n                }\r\n            },\r\n            {\r\n                \"breakpoint\": 550,\r\n                \"settings\": {\r\n                    \"slidesToShow\": 2,\r\n                    \"slidesToScroll\": 1\r\n                }\r\n            }\r\n        ]\r\n    }'\r\n>\r\n    {{#each children}}\r\n\r\n    {{#if image.url}}\r\n    <div data-product-slide class=\"productCarousel-slide\">\r\n        {{> components/category/card }}\r\n    </div>\r\n    {{/if}}\r\n    {{/each}}\r\n</section>\r\n",
  "components/category/card": "<article\r\n    class=\"card\"\r\n    data-cat=\"{{json this}}\"\r\n>\r\n\r\n    <figure class=\"card-figure\">\r\n\r\n        <a href=\"{{path}}\"\r\n           class=\"card-figure__link\"\r\n           data-event-type=\"product-click\"\r\n        >\r\n            <div class=\"card-img\">\r\n             <img src=\"{{image.url}}\" alt=\"\">\r\n            </div>\r\n        </a>\r\n    </figure>\r\n    <div class=\"card-body\">\r\n     <h3 class=\"card-title\">\r\n        <a \r\n           href=\"{{path}}\"\r\n        >\r\n            {{name}}\r\n        </a>\r\n    </h3>\r\n    </div>\r\n</article>\r\n"
}
